# ===== Paths =====
OUTPUT_FILES_PATH=/data/output
EXTRACTED_FILES_PATH=/data/extracted

# ===== Postgres =====
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=rfcnpj

# ===== WebDAV/PROPFIND monthly listing =====
DAV_BASE_DOMAIN=https://arquivos.receitafederal.gov.br
DAV_LIST_URL_TEMPLATE=https://arquivos.receitafederal.gov.br/public.php/dav/files/gn672Ad4CF8N6TK/Dados/Cadastros/CNPJ/%s/

# Month automation:
# - If DB has loaded_month, loader will attempt the next month.
# - If DB is empty, it will start from START_MONTH.
START_MONTH=2026-01
# Force a specific month (YYYY-MM) and skip automation.
FORCE_MONTH=

# ===== Pipeline switches (equivalent to "commented blocks" from Python) =====
ENABLE_DOWNLOAD=true
ENABLE_EXTRACT=true
CREATE_INDEXES=false

# ===== What to load =====
LOAD_EMPRESA=false
LOAD_ESTABELECIMENTO=false
LOAD_SOCIOS=false
LOAD_SIMPLES=false
LOAD_CNAE=false
LOAD_MOTI=false
LOAD_MUNIC=true
LOAD_NATJU=false
LOAD_PAIS=false
LOAD_QUALS=false

# ===== Parallelism =====
DOWNLOAD_WORKERS=4
EXTRACT_WORKERS=2
TABLE_WORKERS=2
FILE_WORKERS=2

# ===== Email notification (SMTP/Gmail) =====
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
MAIL_TO=
MAIL_NOTIFY_UPTODATE=false

# ===== Logging =====
LOG_LEVEL=info
